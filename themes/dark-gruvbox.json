{
  "name": "Gruvbox Dark",
  "mode": "dark",
  "colors": {
    "color-0": "#282828",
    "color-1": "#3c3836",
    "color-2": "#504945",
    "color-3": "#665c54",
    "color-4": "#a89984",
    "color-5": "#ebdbb2",
    "color-6": "#fbf1c7",

    "red": "#fb4934",
    "green": "#b8bb26",
    "yellow": "#fabd2f",
    "blue": "#83a598",
    "purple": "#d3869b",
    "aqua": "#8ec07c",
    "orange": "#fe8019",
    "dark-red": "#cc241d",
    "dark-orange": "#d65d0e",
    "dark-blue": "#458588",
    "dark-yellow": "#d79921",

    "background-base": "var(--color-0)",
    "background-content": "var(--color-1)",
    "background-2": "var(--color-2)",

    "foreground-0": "var(--color-6)",
    "foreground-1": "var(--color-5)",
    "foreground-2": "var(--color-4)",

    "content-0": "var(--color-4)",
    "content-1": "#7c6f64",
    "content-2": "var(--color-2)",

    "border-color": "var(--color-2)",
    "divider-color": "var(--color-3)",

    "active-background-color": "var(--color-2)",
    "active-color": "var(--red)",
    "hover-background-color": "color-mix(in srgb, var(--color-2) 60%, var(--color-3))",
    "hover-color": "var(--yellow)",

    "sidePanel-menu-color": "var(--color-0)",
    "sidePanel-color-hover": "color-mix(in srgb, var(--color-1) 50%, var(--color-2))",
    "sidePanel-color": "color-mix(in srgb, var(--color-0) 50%, var(--color-1))",
    "sidePanel-icon-color": "var(--color-6)",
    "sidePanel-border-color": "var(--color-4)",

    "button-background-color": "transparent",
    "button-color": "var(--color-5)",
    "button-border-color": "#7c6f64",
    "button-hover-background-color": "var(--color-2)",
    "button-hover-color": "var(--color-5)",
    "button-hover-border-color": "var(--dark-orange)",
    "button-active-background-color": "var(--color-3)",
    "button-active-border-color": "var(--color-4)",

    "button-ok-background-color": "var(--dark-orange)",
    "button-ok-color": "var(--color-0)",
    "button-ok-border-color": "var(--dark-orange)",
    "button-ok-hover-background-color": "var(--orange)",
    "button-ok-hover-color": "var(--color-0)",
    "button-ok-hover-border-color": "var(--orange)",
    "button-ok-active-background-color": "var(--dark-orange)",
    "button-ok-active-color": "var(--color-0)",
    "button-ok-active-border-color": "var(--dark-orange)",

    "button-cancel-background-color": "transparent",
    "button-cancel-color": "var(--dark-red)",
    "button-cancel-border-color": "#7c6f64",
    "button-cancel-hover-background-color": "var(--color-1)",
    "button-cancel-hover-color": "var(--red)",
    "button-cancel-hover-border-color": "var(--color-4)",
    "button-cancel-active-background-color": "var(--color-2)",
    "button-cancel-active-color": "var(--dark-red)",
    "button-cancel-active-border-color": "var(--color-4)",

    "input-background-color": "transparent",
    "input-color": "var(--color-5)",
    "input-placeholder-color": "#928374",
    "input-border-color": "var(--color-3)",
    "input-hover-background-color": "var(--color-1)",
    "input-hover-border-color": "var(--dark-orange)",
    "input-focus-border-color": "var(--orange)",

    "link-color": "var(--blue)",
    "link-hover-color": "var(--dark-blue)",

    "highlight-color": "var(--yellow)",
    "accent-color": "var(--dark-orange)",
    "search-list-background-color": "var(--color-1)",
    "search-list-border-color": "var(--color-2)",
    "search-list-item-color": "var(--color-4)",
    "search-list-item-active-color": "var(--color-2)",
    "search-list-highlight-color": "var(--dark-yellow)",

    "scrollbar-background-color": "var(--color-0)",
    "scrollbar-thumb-color": "var(--color-3)",
    "scrollbar-thumb-hover-color": "#7c6f64",
    "scrollbar-border-color": "var(--color-1)",

    "tab-color": "var(--color-4)",
    "tab-background-color": "transparent",
    "tab-selected-color": "var(--color-5)",
    "tab-selected-background-color": "var(--color-2)",
    "tab-hover-background-color": "var(--color-1)",

    "popup-background-color": "var(--color-0)",
    "popup-color": "var(--color-5)",
    "popup-border-color": "var(--color-2)",

    "dialog-background-color": "var(--color-0)",
    "dialog-color": "var(--color-5)",
    "dialog-border-color": "var(--color-2)",
    "dialog-footer-background-color": "var(--color-1)",

    "transparent-bg": "rgba(40, 40, 40, 0.8)",

    "success-background-color": "rgba(184, 187, 38, 0.2)",
    "error-background-color": "rgba(204, 36, 29, 0.2)",
    "warning-background-color": "rgba(2FAB, 189, 47, 0.2)",
    "info-background-color": "rgba(131, 165, 152, 0.2)",

    "status-200-color": "var(--green)",
    "status-300-color": "var(--yellow)",
    "status-400-color": "var(--red)",
    "status-500-color": "var(--dark-red)",

    "method-get-color": "var(--green)",
    "method-post-color": "var(--blue)",
    "method-put-color": "var(--yellow)",
    "method-delete-color": "var(--red)",
    "method-patch-color": "var(--purple)",

    "json-key-color": "var(--yellow)",
    "json-string-color": "var(--green)",
    "json-number-color": "var(--purple)",
    "json-boolean-color": "var(--orange)",
    "json-null-color": "#928374",
    "console-background-color": "var(--color-0)",
    "console-color": "var(--color-5)",
    "switch-active-background-color": "var(--color-3)",
    "switch-active-color": "var(--yellow)",
    "switch-background-color": "var(--color-2)",
    "switch-color": "var(--color-5)",
    "switch-hover-color": "var(--dark-yellow)",
    "switch-text-active-color": "var(--color-6)",
    "switch-text-color": "var(--color-4)",
    "switch-text-hover-color": "var(--color-6)"
  },
  "editor": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      { "token": "", "foreground": "ebdbb2", "background": "282828" },
      { "token": "comment", "foreground": "928374" },
      { "token": "string", "foreground": "b8bb26" },
      { "token": "keyword", "foreground": "fb4934" },
      { "token": "number", "foreground": "d3869b" },
      { "token": "string.key.json", "foreground": "fabd2f" },
      { "token": "string.value.json", "foreground": "b8bb26" }
    ],
    "colors": {
      "editor.foreground": "#ebdbb2",
      "editor.background": "#282828",
      "editor.selectionBackground": "#504945",
      "editor.lineHighlightBackground": "#3c3836",
      "editorCursor.foreground": "#ebdbb2",
      "editorWhitespace.foreground": "#504945"
    }
  }
}
