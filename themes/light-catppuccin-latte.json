{
  "name": "Catppuccin Latte",
  "mode": "light",
  "colors": {
    "color-0": "#eff1f5",
    "color-1": "#e6e9ef",
    "color-2": "#dce0e8",
    "color-3": "#ccd0da",
    "color-4": "#bcc0cc",
    "color-5": "#9ca0b0",
    "color-6": "#4c4f69",
    "rosewater": "#dc8a78",
    "flamingo": "#dd7878",
    "pink": "#ea76cb",
    "red": "#d20f39",
    "maroon": "#e64553",
    "peach": "#fe640b",
    "yellow": "#df8e1d",
    "green": "#40a02b",
    "teal": "#179299",
    "sky": "#04a5e5",
    "sapphire": "#209fb5",
    "blue": "#1e66f5",
    "lavender": "#7287fd",

