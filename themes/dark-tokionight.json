{
  "name": "Tokio Night",
  "mode": "dark",
  "colors": {
    "color-0": "#16161f",
    "color-1": "#1a1b27",
    "color-2": "#1f202f",
    "color-3": "#313142",
    "color-4": "#474967",
    "color-5": "#565f89",
    "color-6": "#c0caf5",
    "yellow": "#e0af68",
    "red": "#f7768e",
    "magenta": "#f14e7a",
    "violet": "#9d7cd8",
    "blue": "#7aa2f7",
    "cyan": "#7dcfff",
    "green": "#9ece6a",
    "accent-color": "var(--blue)",
    "active-background-color": "var(--color-2)",
