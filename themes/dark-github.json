{
  "name": "GitHub Dark",
  "mode": "dark",
  "colors": {
    "color-0": "#0d1117",
    "color-1": "#161b22",
    "color-2": "#21262d",
    "color-3": "#30363d",
    "color-4": "#8b949e",
    "color-5": "#c9d1d9",
    "color-6": "#f0f6fc",

    "blue": "#58a6ff",
    "green": "#238636",
    "green-light": "#2ea043",
    "green-bright": "#3fb950",
    "red": "#f85149",
    "orange": "#db6d28",
    "yellow": "#d29922",
    "purple": "#bc8cff",

    "background-base": "var(--color-0)",
    "background-content": "var(--color-1)",
    "background-2": "var(--color-2)",

    "foreground-0": "var(--color-5)",
    "foreground-1": "var(--color-4)",
    "foreground-2": "#484f58",

    "content-0": "var(--color-4)",
    "content-1": "var(--color-3)",
    "content-2": "var(--color-2)",

    "border-color": "color-mix(in srgb, var(--color-2) 50%, var(--color-3))",
    "divider-color": "var(--border-color)",

    "active-background-color": "var(--color-2)",
    "active-color": "var(--blue)",
    "hover-background-color": "var(--color-2)",
    "hover-color": "var(--blue)",

    "sidePanel-menu-color": "var(--color-0)",
    "sidePanel-color-hover": "var(--color-2)",
    "sidePanel-color": "color-mix(in srgb, var(--color-0) 50%, var(--color-1))",
    "sidePanel-icon-color": "var(--foreground-1)",

    "button-background-color": "var(--color-2)",
    "button-color": "var(--color-5)",
    "button-border-color": "var(--color-3)",
    "button-hover-background-color": "var(--color-3)",
    "button-hover-color": "var(--color-5)",
    "button-hover-border-color": "var(--color-4)",
    "button-active-background-color": "var(--color-2)",
    "button-active-border-color": "var(--color-4)",

    "button-ok-background-color": "var(--green)",
    "button-ok-color": "#ffffff",
    "button-ok-border-color": "var(--green-light)",
    "button-ok-hover-background-color": "var(--green-light)",
    "button-ok-hover-color": "#ffffff",
    "button-ok-hover-border-color": "var(--green-bright)",
    "button-ok-active-background-color": "var(--green)",
    "button-ok-active-color": "#ffffff",
    "button-ok-active-border-color": "var(--green-bright)",

    "button-cancel-background-color": "var(--color-2)",
    "button-cancel-color": "var(--red)",
    "button-cancel-border-color": "var(--color-3)",
    "button-cancel-hover-background-color": "var(--color-3)",
    "button-cancel-hover-color": "var(--red)",
    "button-cancel-hover-border-color": "var(--color-3)",
    "button-cancel-active-background-color": "var(--color-2)",
    "button-cancel-active-color": "var(--red)",
    "button-cancel-active-border-color": "var(--color-3)",

    "input-background-color": "transparent",
    "input-color": "var(--color-5)",
    "input-placeholder-color": "var(--foreground-2)",
    "input-border-color": "var(--color-3)",
    "input-hover-background-color": "var(--color-1)",
    "input-hover-border-color": "var(--blue)",
    "input-focus-border-color": "var(--blue)",

    "link-color": "var(--blue)",
    "link-hover-color": "#79c0ff",

    "highlight-color": "#1f6feb",
    "accent-color": "var(--blue)",
    "search-list-background-color": "var(--color-1)",
    "search-list-border-color": "var(--color-3)",
    "search-list-item-color": "var(--color-5)",
    "search-list-item-active-color": "var(--color-2)",
    "search-list-highlight-color": "#1f6feb",

    "scrollbar-background-color": "var(--color-0)",
    "scrollbar-thumb-color": "var(--color-3)",
    "scrollbar-thumb-hover-color": "var(--foreground-2)",
    "scrollbar-border-color": "var(--color-2)",

    "tab-color": "var(--color-4)",
    "tab-background-color": "transparent",
    "tab-selected-color": "var(--color-5)",
    "tab-selected-background-color": "var(--color-1)",
    "tab-hover-background-color": "var(--color-2)",

    "popup-background-color": "var(--color-1)",
    "popup-color": "var(--color-5)",
    "popup-border-color": "var(--color-3)",

    "dialog-background-color": "var(--color-0)",
    "dialog-color": "var(--color-5)",
    "dialog-border-color": "var(--color-3)",
    "dialog-footer-background-color": "var(--color-1)",

    "transparent-bg": "rgba(13, 17, 23, 0.8)",

    "success-background-color": "rgba(46, 160, 67, 0.2)",
    "error-background-color": "rgba(248, 81, 73, 0.2)",
    "warning-background-color": "rgba(210, 153, 34, 0.2)",
    "info-background-color": "rgba(88, 166, 255, 0.2)",

    "status-200-color": "var(--green-bright)",
    "status-300-color": "var(--yellow)",
    "status-400-color": "var(--red)",
    "status-500-color": "var(--red)",

    "method-get-color": "var(--green-bright)",
    "method-post-color": "var(--blue)",
    "method-put-color": "var(--yellow)",
    "method-delete-color": "var(--red)",
    "method-patch-color": "var(--orange)",

    "console-background-color": "var(--color-0)",
    "console-color": "var(--color-5)",

    "switch-text-color": "var(--foreground-1)",
    "switch-text-hover-color": "#79c0ff",
    "switch-text-active-color": "var(--blue)",
    "switch-active-background-color": "var(--color-3)",
    "switch-active-color": "var(--blue)",
    "switch-background-color": "var(--color-2)",
    "switch-color": "var(--color-6)",
    "switch-hover-color": "#79c0ff"
  },
  "editor": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      { "token": "keyword", "foreground": "var(--purple)" },
      { "token": "operator", "foreground": "var(--purple)" },
      { "token": "variable", "foreground": "var(--foreground-1)" },
      { "token": "variable.parameter", "foreground": "var(--foreground-1)" },
      { "token": "function", "foreground": "var(--blue)" },
      { "token": "method", "foreground": "var(--blue)" },
      { "token": "type", "foreground": "var(--yellow)" },
      { "token": "class", "foreground": "var(--yellow)" },
      { "token": "interface", "foreground": "var(--yellow)" },
      { "token": "string.link", "foreground": "var(--blue)" },
      { "token": "strong", "foreground": "var(--foreground-0)", "fontStyle": "bold" },
      { "token": "emphasis", "foreground": "var(--foreground-1)", "fontStyle": "italic" },
      { "token": "", "foreground": "c9d1d9", "background": "0d1117" },
      { "token": "comment", "foreground": "8b949e" },
      { "token": "string", "foreground": "a5d6ff" },
      { "token": "keyword", "foreground": "ff7b72" },
      { "token": "number", "foreground": "79c0ff" },
      { "token": "string.key.json", "foreground": "7ee787" },
      { "token": "string.value.json", "foreground": "a5d6ff" }
    ],
    "colors": {
      "editor.foreground": "#c9d1d9",
      "editor.background": "#0d1117",
      "editor.selectionBackground": "#1f6feb",
      "editor.lineHighlightBackground": "#161b22",
      "editorCursor.foreground": "#c9d1d9",
      "editorWhitespace.foreground": "#21262d"
    }
  }
}
