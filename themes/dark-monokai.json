    "text-warning": "var(--color-7)",
    "button-active-background-color": "var(--color-4)",
    "button-active-border-color": "var(--color-5)",
    "button-active-color": "var(--color-6)",
    "button-background-color": "transparent",
    "button-border-color": "var(--color-4)",
    "button-cancel-active-background-color": "var(--color-3)",
    "button-cancel-active-border-color": "var(--color-5)",
    "button-cancel-active-color": "var(--color-6)",
    "button-cancel-background-color": "transparent",
    "button-cancel-border-color": "var(--color-3)",
    "button-cancel-color": "var(--color-4)",
    "button-cancel-hover-background-color": "var(--color-2)",
    "button-cancel-hover-border-color": "var(--color-4)",
    "button-cancel-hover-color": "var(--color-5)",
    "button-color": "var(--color-6)",
    "button-focus-color": "var(--color-6)",
    "button-hover-background-color": "var(--color-3)",
    "button-hover-border-color": "var(--color-5)",
    "button-hover-color": "var(--color-6)",
    "button-ok-active-background-color": "var(--color-4)",
    "button-ok-active-border-color": "var(--color-6)",
    "button-ok-active-color": "var(--color-6)",
    "button-ok-background-color": "var(--color-2)",
    "button-ok-border-color": "var(--color-4)",
    "button-ok-color": "var(--color-6)",
    "button-ok-hover-background-color": "var(--color-3)",
    "button-ok-hover-border-color": "var(--color-5)",
    "button-ok-hover-color": "var(--color-6)",
    "cancel-color": "rgba(249, 38, 114, 0.1)",
    "console-background-color": "var(--color-0)",
    "console-color": "var(--color-5)",
    "danger-color": "#a78",
    "dialog-background-color": "var(--color-0)",
    "dialog-border-color": "var(--color-3)",
    "dialog-color": "var(--color-6)",
    "dialog-footer-background-color": "var(--color-1)",
    "dialog-footer-color": "var(--color-5)",
    "dialog-footer-hover-color": "var(--color-6)",
    "droppable-color": "var(--color-5)",
    "error-background-color": "rgba(249, 38, 114, 0.3)",
    "error-color": "var(--color-8)",
    "gutter-color-drag": "var(--color-8)",
    "gutter-color": "var(--border-color)",
    "highlight-color": "var(--color-9)",
    "hover-background-color": "var(--color-2)",
    "hover-color": "var(--color-6)",
    "icon-color": "var(--color-6)",
    "input-background-color": "transparent",
    "input-border-color": "var(--color-4)",
    "input-color": "var(--color-6)",
    "input-disabled-color": "var(--color-5)",
    "input-hover-background-color": "var(--color-2)",
    "input-hover-border-color": "var(--color-5)",
    "input-hover-color": "var(--color-6)",
    "input-placeholder-color": "var(--color-5)",
    "input-undefined-color": "var(--color-5)",
    "link-color": "var(--color-5)",
    "link-hover-color": "var(--color-6)",
    "loading-color": "var(--color-5)",
    "method-delete-color": "var(--color-8)",
    "method-get-color": "var(--color-10)",
    "method-head-color": "var(--color-5)",
    "method-option-color": "var(--color-11)",
    "method-patch-color": "#e6db74",
    "method-post-color": "var(--color-9)",
    "method-put-color": "var(--color-7)",
    "overlay-color": "rgba(0, 0, 0, 0.45)",
    "response-no-content-background-color": "var(--color-0)",
    "response-no-content-color": "var(--color-5)",
    "scrollbar-background-color": "var(--background-base)",
    "scrollbar-border-color": "var(--border-color)",
    "scrollbar-corner-color": "var(--color-1)",
    "scrollbar-thumb-color": "var(--border-color)",
    "scrollbar-thumb-hover-color": "var(--color-4)",
    "search-list-background-color": "var(--background-content)",
    "search-list-border-color": "var(--border-color)",
    "search-list-highlight-color": "#66d9ef44",
    "search-list-item-active-color": "var(--active-background-color)",
    "search-list-item-color-2": "var(--color-5)",
    "search-list-item-color-3": "var(--color-7)",
    "search-list-item-color": "var(--color-6)",
    "search-list-item-hover-color": "var(--hover-background-color)",
    "search-list-no-results-color": "var(--color-6)",
    "select-arrow-color": "var(--color-5)",
    "select-arrow": "var(--base-color)",
    "selected-color": "var(--color-1)",
    "sidePanel-color-hover": "var(--color-3)",
    "sidePanel-color": "color-mix(in srgb, var(--color-1) 60%, var(--color-2))",
    "sidePanel-icon-color": "var(--color-6)",
    "status-200-color": "var(--color-10)",
    "status-300-color": "var(--color-7)",
    "status-400-color": "var(--color-11)",
    "status-500-color": "var(--color-8)",
    "status-unkonwn-color": "var(--color-5)",
    "success-background-color": "var(--color-10)",
    "switch-active-background-color": "var(--color-3)",
    "switch-active-color": "var(--color-10)",
    "switch-background-color": "var(--color-3)",
    "switch-color": "var(--color-5)",
    "switch-hover-color": "var(--color-6)",
    "switch-text-active-color": "var(--color-6)",
    "switch-text-color": "var(--color-5)",
    "switch-text-hover-color": "var(--color-6)",
    "tab-background-color": "transparent",
    "tab-color": "var(--base-color)",
    "tab-hover-background-color": "var(--color-2)",
    "tab-panel-background-color": "var(--color-0)",
    "tab-selected-background-color": "var(--color-3)",
    "tab-selected-color": "var(--base-color)",
    "table-border-color": "var(--border-color)",
    "blue": "var(--color-9)",
    "foreground-0": "var(--color-6)",
    "foreground-1": "var(--color-5)",
    "purple": "var(--color-8)"
  },
  "editor": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      {
        "token": "keyword",
        "foreground": "var(--purple)"
      },
      {
        "token": "operator",
        "foreground": "var(--purple)"
      },
      {
        "token": "variable",
        "foreground": "var(--foreground-1)"
      },
      {
        "token": "variable.parameter",
        "foreground": "var(--foreground-1)"
      },
      {
        "token": "function",
        "foreground": "var(--blue)"
      },
      {
        "token": "method",
        "foreground": "var(--blue)"
      },
      {
        "token": "type",
        "foreground": "var(--yellow)"
      },
      {
        "token": "class",
        "foreground": "var(--yellow)"
      },
      {
        "token": "interface",
        "foreground": "var(--yellow)"
      },
      {
        "token": "string.link",
        "foreground": "var(--blue)"
      },
      {
        "token": "strong",
        "foreground": "var(--foreground-0)",
        "fontStyle": "bold"
      },
      {
        "token": "emphasis",
        "foreground": "var(--foreground-1)",
        "fontStyle": "italic"
      },
      {
        "background": "272822",
        "foreground": "F8F8F2",
        "token": ""
      },
      {
        "foreground": "75715e",
        "token": "comment"
      },
      {
        "foreground": "f92672",
        "token": "string.key.json"
      },
      {
        "foreground": "ae81ff",
        "token": "keyword.json"
      },
      {
        "foreground": "a6e22e",
        "token": "number.json"
      },
      {
        "foreground": "fff48e",
        "token": "string.value.json"
      }
    ],
    "colors": {
      "editor.foreground": "#F8F8F2",
      "editor.background": "#272822",
      "editor.selectionBackground": "#49483E",
      "editor.lineHighlightBackground": "#3E3D32",
      "editorCursor.foreground": "#F8F8F0",
      "editorWhitespace.foreground": "#3B3A32",
      "editorIndentGuide.activeBackground": "#9D550FB0",
      "editor.selectionHighlightBorder": "#222218"
    }
  }
}
