{
  "name": "Monokai",
  "mode": "dark",
  "colors": {
    "color-0": "#1e1f19",
    "color-1": "#272822",
    "color-2": "#3e3d32",
    "color-3": "#49483e",
    "color-4": "#75715e",
    "color-5": "#a59f85",
    "color-6": "#f8f8f2",
    "color-7": "#fd971f",
    "color-8": "#f92672",
    "color-9": "#66d9ef",
    "color-10": "#a6e22e",
    "color-11": "#ae81ff",
    "accent-color": "var(--color-6)",
    "active-background-color": "var(--color-2)",
    "active-color": "var(--color-6)",
    "background-base": "var(--color-1)",
