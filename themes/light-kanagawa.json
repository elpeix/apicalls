  "editor": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "background": "f7f1e5",
        "foreground": "504c6a",
        "token": ""
      },
      {
        "foreground": "777777",
        "token": "comment"
      },
      {
        "foreground": "dca561",
        "token": "keyword.json"
      },
      {
        "foreground": "504c6a",
        "token": "string.key.json"
      },
      {
        "foreground": "7e9cd8",
        "token": "number.json"
      },
      {
        "foreground": "76946a",
        "token": "string.value.json"
      }
    ],
    "colors": {
      "editor.foreground": "#504C6A",
      "editor.background": "#fefcef",
      "editor.selectionBackground": "#e5d7ba",
      "editor.lineHighlightBackground": "#ebe2d0",
      "editorCursor.foreground": "#504C6A",
      "editorWhitespace.foreground": "#363646",
      "editorIndentGuide.activeBackground": "#dca561",
      "editor.selectionHighlightBorder": "#504C6A"
    }
  }
}
