  "editor": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      {
        "background": "24273a",
        "foreground": "cad3f5",
        "token": ""
      },
      {
        "foreground": "a5adcb",
        "token": "comment"
      },
      {
        "foreground": "8bd5ca",
        "token": "string.key.json"
      },
      {
        "foreground": "a5adcb",
