{
  "name": "Pure grey",
  "mode": "light",
  "colors": {
    "foreground-0": "#343a40",
    "foreground-1": "#495057",
    "foreground-2": "#6c757d",
    "content-0": "#adb5bd",
    "content-1": "#b8bfc5",
    "content-2": "#c8d0d6",
    "background-2": "#e8ebee",
    "background-1": "#f2f4f7",
    "background-0": "#fafbfc",
    "yellow": "#bdc5cd",
    "orange": "#adb5bd",
    "red": "#343a40",
    "magenta": "#596067",
    "violet": "#495057",
    "blue": "#6c757d",
    "cyan": "#a8afb5",
    "green": "#5c656d",
    "transparent-red": "rgba(100, 100, 100, 0.1)",
    "transparent-cyan": "rgba(160, 160, 160, 0.2)",
    "transparent-bg": "rgba(0, 0, 0, 0.5)"
  },
  "editor": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "background": "343a40",
        "foreground": "ffffff",
        "token": ""
      },
      {
        "foreground": "b8bfc5",
        "token": "comment"
      },
      {
        "foreground": "343a40",
        "token": "string.key.json"
      },
      {
        "foreground": "495057",
        "token": "keyword.json"
      },
      {
        "foreground": "596067",
        "token": "number.json"
      },
      {
        "foreground": "6c757d",
        "token": "string.value.json"
      }
    ],
    "colors": {
      "editor.foreground": "#343a40",
      "editor.background": "#ffffff",
      "editor.selectionBackground": "#c8d0d6",
      "editor.lineHighlightBackground": "#e8ebee",
      "editorCursor.foreground": "#495057",
      "editorWhitespace.foreground": "#c8d0d6",
      "editorIndentGuide.activeBackground": "#adb5bd",
      "editor.selectionHighlightBorder": "#e8ebee"
    }
  }
}
